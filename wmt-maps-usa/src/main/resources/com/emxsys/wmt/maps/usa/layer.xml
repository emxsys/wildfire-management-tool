<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="WorldWind">
        <folder name="Layers">
            <file name="USDANAIP.instance">
                <attr name="name" stringvalue="USDA NAIP"/>
                <attr name="type" stringvalue="Raster"/>
                <attr name="role" stringvalue="Basemap"/>
                <attr name="category" stringvalue="Aerial"/>
                <attr name="actuate" stringvalue="onRequest"/>
                <attr name="displayName" bundlevalue="com.emxsys.wmt.maps.usa.Bundle#LAYER_USDANAIP"/>
                <attr name="position" intvalue="600"/>
                <!-- Look for the config file resource on the classpath - e.g. worldwind.jar-->
                <attr name="config" urlvalue="nbres:/worldwind/config/USA/USDANAIPWMSImageLayer.xml"/>
                <attr name="instanceClass" stringvalue="gov.nasa.worldwind.layers.Layer"/>
                <attr name="instanceCreate" methodvalue="com.emxsys.wmt.globe.layers.LayerFactory.createLayer"/>
            </file>
            
