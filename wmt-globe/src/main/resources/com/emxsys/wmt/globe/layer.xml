<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="WorldWind">

        
        <!--Categories are used to group layers within the menu system-->
        <folder name="Categories">
            <file name="Background">                
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#Categories.Background"/>
                <attr name="iconBase" stringvalue="com/emxsys/wmt/globe/imageslayer-wms.png"/>
                <attr name="position" intvalue="100"/>
            </file>
            <file name="Satellite">                
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#Categories.Satellite"/>
                <attr name="iconBase" stringvalue="com/emxsys/wmt/globe/imageslayer-gps.png"/>
                <attr name="position" intvalue="200"/>
            </file>
            <file name="Aerial">                
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#Categories.Aerial"/>
                <attr name="iconBase" stringvalue="com/emxsys/wmt/globe/imageslayer-raster.png"/>
                <attr name="position" intvalue="300"/>
            </file>
            <file name="Topographic">                
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#Categories.Topographic"/>
                <attr name="iconBase" stringvalue="com/emxsys/wmt/globe/imageslayer-raster-profile.png"/>
                <attr name="position" intvalue="400"/>
                <attr name="priority" stringvalue="top"/>
            </file>
            <file name="Other">                
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#Categories.Other"/>
                <attr name="iconBase" stringvalue="com/emxsys/wmt/globe/imageslayer-wfs.png"/>
                <attr name="position" intvalue="500"/>
            </file>
            <file name="Places">                
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#Categories.Places"/>
                <attr name="iconBase" stringvalue="com/emxsys/wmt/globe/imageslayer-label.png"/>
                <attr name="position" intvalue="600"/>
            </file>
        </folder>
            
        <folder name="Layers">
             <file name="Graphics.instance">
                <attr name="name" stringvalue="Graphics"/>
                <attr name="type" stringvalue="Vector"/>
                <attr name="role" stringvalue="Data"/>
                <attr name="category" stringvalue="Other"/>
                <attr name="actuate" stringvalue="onLoad"/>
                <attr name="opacity" doublevalue="0.8"/>
                <attr name="position" intvalue="1249"/>
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#WorldWind/Layers/Graphics.instance"/>
                <attr name="instanceClass" stringvalue="com.emxsys.wmt.globe.layers.GraphicLayer"/>
                <attr name="instanceCreate" methodvalue="com.emxsys.wmt.globe.layers.GraphicLayer.getInstance"/>
            </file>
            <file name="Symbols.instance">
                <attr name="name" stringvalue="Symbols"/>
                <attr name="type" stringvalue="Vector"/>
                <attr name="role" stringvalue="Symbology"/>
                <attr name="category" stringvalue="Other"/>
                <attr name="actuate" stringvalue="onLoad"/>
                <attr name="opacity" doublevalue="0.8"/>
                <attr name="position" intvalue="1250"/>
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#WorldWind/Layers/Symbols.instance"/>
                <attr name="instanceClass" stringvalue="com.emxsys.wmt.globe.layers.SymbolLayer"/>
                <attr name="instanceCreate" methodvalue="com.emxsys.wmt.globe.layers.SymbolLayer.getInstance"/>
            </file>
            <file name="Shapes.instance">
                <attr name="name" stringvalue="Shapes"/>
                <attr name="type" stringvalue="Vector"/>
                <attr name="role" stringvalue="Data"/>
                <attr name="category" stringvalue="Other"/>
                <attr name="actuate" stringvalue="onLoad"/>
                <attr name="opacity" doublevalue="0.65"/>
                <attr name="position" intvalue="1275"/>
                <attr name="displayName" bundlevalue="com.emxsys.wmt.globe.Bundle#WorldWind/Layers/Shapes.instance"/>
                <attr name="instanceClass" stringvalue="com.emxsys.wmt.globe.layers.ShapesLayer"/>
                <attr name="instanceCreate" methodvalue="com.emxsys.wmt.globe.layers.ShapesLayer.getInstance"/>
            </file>
        </folder>
    </folder>
</filesystem>
