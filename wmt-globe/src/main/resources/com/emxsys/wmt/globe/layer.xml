<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Loaders">
    </folder>
    <folder name="Ribbon">
        <folder name="TaskPanes"> 
            <!-- Marker Tools - CONTEXTUAL TAB -->
            <folder name="MarkerTools">
                <attr name="position" intvalue="100"/> 
                <attr name="menuText" stringvalue="Marker"/>
                <attr name="contextualTaskGroup" bundlevalue="com.emxsys.wmt.globe.ribbons.Bundle#CTL_MarkerTools"/>
                    
                <folder name="Edit">
                    <attr name="position" intvalue="100"/>
                    <attr name="menuText" stringvalue="Edit"/>
                </folder> 
                <folder name="View">
                    <attr name="position" intvalue="200"/>
                    <attr name="menuText" stringvalue="View"/>
                </folder> 
            </folder>
            <!-- Symbology Tools - CONTEXTUAL TAB -->
            <folder name="SymbolTools">
                <attr name="position" intvalue="100"/> 
                <attr name="menuText" stringvalue="Symbol"/>
                <attr name="contextualTaskGroup" bundlevalue="com.emxsys.wmt.globe.ribbons.Bundle#CTL_SymbolTools"/>
                    
                <folder name="Edit">
                    <attr name="position" intvalue="100"/>
                    <attr name="menuText" stringvalue="Edit"/>
                </folder> 
                <folder name="Affiliation">
                    <attr name="position" intvalue="200"/>
                    <attr name="menuText" stringvalue="Affiliation"/>
                </folder> 
                <folder name="Status">
                    <attr name="position" intvalue="300"/>
                    <attr name="menuText" stringvalue="Status"/>
                </folder> 
                <folder name="View">
                    <attr name="position" intvalue="400"/>
                    <attr name="menuText" stringvalue="View"/>
                </folder> 
            </folder>
            <!-- Scene Tools - CONTEXTUAL TAB -->
            <folder name="SceneTools">
                <attr name="position" intvalue="300"/> 
                <attr name="menuText" stringvalue="Scene"/>
                <attr name="contextualTaskGroup" bundlevalue="com.emxsys.wmt.globe.ribbons.Bundle#CTL_SceneTools"/>
                    
                <folder name="Edit">
                    <attr name="position" intvalue="100"/>
                    <attr name="menuText" stringvalue="Change"/>
                    <file name="com-emxsys-wmt-globe-scene-rename.instance">
                        <attr name="instanceClass" stringvalue="org.openide.actions.RenameAction"/>
                        <attr name="displayName" stringvalue="Rename"/>
                        <attr name="description" stringvalue="Rename the selected scene."/>
                        <attr name="tooltipTitle" stringvalue="Rename Scene"/>
                        <attr name="tooltipBody" stringvalue="Renames the scene using an editor."/>
                        <attr name="tooltipIcon" stringvalue="com/emxsys/wmt/core/images/edit32.png"/>
                        <attr name="iconBase" stringvalue="com/emxsys/wmt/core/images/edit.png"/>    
                        <attr name="position" intvalue="100"/>
                    </file>
                    <file name="com-emxsys-wmt-globe-scene-delete.instance">
                        <attr name="instanceClass" stringvalue="org.openide.actions.DeleteAction"/>
                        <attr name="displayName" stringvalue="Delete"/>
                        <attr name="description" stringvalue="Delete the selected scene."/>
                        <attr name="tooltipTitle" stringvalue="Delete Scene"/>
                        <attr name="tooltipBody" stringvalue="Deletes the selected scene after confirmation."/>
                        <attr name="tooltipIcon" stringvalue="com/emxsys/wmt/core/images/delete32.png"/>
                        <attr name="iconBase" stringvalue="com/emxsys/wmt/core/images/delete.png"/>    
                        <attr name="position" intvalue="200"/>
                    </file>
                </folder> 
                <folder name="View">
                    <attr name="position" intvalue="200"/>
                    <attr name="menuText" stringvalue="View"/>
                </folder> 
            </folder>
        </folder>                
    </folder>
